<?xml version="1.0" ?>
<odoo>
    <record id="module_transport_category" model="ir.module.category">
        <field name="name">Transport</field>
        <field name="sequence">18</field>
    </record>
    <record id="transportation_executive_group_user" model="res.groups">
        <field name="name">Transportation Executive</field>
        <field name="category_id" ref="module_transport_category"/>
        <field name="implied_ids" eval="[(4, ref('base.group_user'))]"/>
    </record>
    <record id="traffic_executive_group_user" model="res.groups">
        <field name="name">Traffic Executive</field>
        <field name="category_id" ref="module_transport_category"/>
        <field name="implied_ids" eval="[(4, ref('transportation_executive_group_user'))]"/>
        <field name="users" eval="[(4, ref('base.user_root')), (4, ref('base.user_admin'))]"/>
    </record>
</odoo>
