<?xml version="1.0" encoding="utf-8"?>
<odoo>

    <record id="view_partner_form" model="ir.ui.view">
        <field name="name">view.partner.form</field>
        <field name="model">res.partner</field>
        <field name="inherit_id" ref="base.view_partner_form"/>
        <field name="arch" type="xml">
            <xpath expr="//div[@class='oe_title mb24']" position="inside">
                <label for="is_customs_house" invisible="1"/>
                <field name="is_customs_house" nolabel="1" invisible="1"/>
                <!-- <br></br> -->
                <label for="custom_house_ref" attrs="{'invisible': [('is_customs_house','=',False)]}"/>
                <field name="custom_house_ref" nolabel="1" attrs="{'invisible': [('is_customs_house','=',False)]}"/>
            </xpath>
        </field>
    </record>

</odoo>
