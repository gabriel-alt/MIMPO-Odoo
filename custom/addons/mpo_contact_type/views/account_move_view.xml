<?xml version="1.0" encoding="utf-8"?>
<odoo>

	<record id="view_move_form" model="ir.ui.view">
		<field name="name">view_move_form.mpo_contact_type</field>
		<field name="model">account.move</field>
		<field name="inherit_id" ref="account.view_move_form"/>
		<field name="arch" type="xml">
			<xpath expr="//form/sheet/group/group[2]/field[@name='currency_id']" position="after">
				<field name="custom_broker_id"/>
			</xpath>
			<xpath expr="//page[@id='invoice_tab']/field[@name='invoice_line_ids']/tree/field[@name='analytic_distribution']" position="after">
				<field name="custom_broker_id" optional="show"/>
			</xpath>
			<xpath expr="//page[@id='aml_tab']/field[@name='line_ids']/tree/field[@name='analytic_distribution']" position="after">
				<field name="custom_broker_id" optional="show"/>
			</xpath>
		</field>
	</record>

</odoo>
