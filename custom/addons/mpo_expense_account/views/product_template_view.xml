<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <record id="product_template_form_view_mpo_expense_account" model="ir.ui.view">
        <field name="name">product.template.form.view.accounting</field>
        <field name="model">product.template</field>
        <field name="inherit_id" ref="account.product_template_form_view"/>
        <field name="arch" type="xml">
            <xpath expr="//page[@name='invoicing']/group[@name='properties']/group[@name='payables']/field[@name='property_account_expense_id']" position="after">
                <field name="property_payment_third_party_account_id"/>
            </xpath>
        </field>
    </record>
</odoo>