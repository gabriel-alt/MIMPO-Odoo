<?xml version="1.0" encoding="utf-8"?>
<odoo>

	<record id="view_tax_form" model="ir.ui.view">
		<field name="name">view_tax_form.mpo_invoice_report</field>
		<field name="model">account.tax</field>
		<field name="inherit_id" ref="account.view_tax_form"/>
		<field name="arch" type="xml">
			<xpath expr="//group[@name='advanced_booleans']" position="after">
				<group string="Data for Report Invoice">
					<field name="impuesto_tax_code"/>
					<field name="impuesto_tax_name"/>
				</group>
			</xpath>
		</field>
	</record>

</odoo>
