<odoo>
    <record id="custody_type_view_tree" model="ir.ui.view">
        <field name="name">custody.type.view.tree</field>
        <field name="model">custody.type</field>
        <field name="arch" type="xml">
            <tree editable="top">
                <field name="name"/>
            </tree>
        </field>
    </record>
    <record id="custody_type_action" model="ir.actions.act_window">
        <field name="name">Custody Types</field>
        <field name="res_model">custody.type</field>
        <field name="view_mode">tree</field>
    </record>
</odoo>
