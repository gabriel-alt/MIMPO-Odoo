<odoo>
    <record id="l10n_mx_edi_inh_res_partner_form" model="ir.ui.view">
        <field name="name">res.partner.form.l10n_mx_edi.addenda</field>
        <field name="model">res.partner</field>
        <field name="inherit_id" ref="l10n_mx_edi.l10n_mx_edi_inh_res_partner_form" />
        <field name="arch" type="xml">
            <xpath expr="//field[@name='l10n_mx_edi_addenda']" position="before">
                <field name="cortefiel_vendor"/>
                <field name="addenda_pedimento"/>
            </xpath>
            <xpath expr="//field[@name='l10n_mx_edi_addenda']" position="attributes">
                <attribute name="invisible">1</attribute>
            </xpath>
        </field>
    </record>
</odoo>
